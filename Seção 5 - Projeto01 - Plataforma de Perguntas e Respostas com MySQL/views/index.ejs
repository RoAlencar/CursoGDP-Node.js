    <p>Quantidade de inscritos: <%= inscritos %></p>

    <%if(msg == true) {%>
    <h3> Isso Ã© uma msg de erro!</h3>
    <%} else {%>
    <h3>Nenhum erro </h3>
    <%}%>

    <% produtos.forEach(function(produto) { %>
    <hr>
    <h3> <%= produto.nome %> </h3>
    <h4> <%= produto.preco %> </h4>       
    <% }) %>
</body>
</html>